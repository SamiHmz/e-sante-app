            android:layout_marginTop="15dp"
            android:gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Button20">

            <Button
                android:id="@+id/Button211"
                android:layout_width="63dp"
                android:layout_height="40dp"
                android:layout_margin="2dp"
                android:background="@drawable/arrondi"
                android:fontFamily="@font/maax_rounded"
                android:padding="5dp"
                android:text="Fatigue"
                android:textSize="12sp" />

            <Button
                android:id="@+id/Button212"
                android:layout_width="58dp"
                android:layout_height="40dp"
                android:layout_margin="2dp"
                android:background="@drawable/arrondi"
                android:fontFamily="@font/maax_rounded"
                android:padding="5dp"
                android:text="Fièvre"
                android:textSize="12sp" />

            <Button
                android:id="@+id/Button23"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_margin="2dp"
                android:background="@drawable/arrondi"
                android:fontFamily="@font/maax_rounded"
                android:padding="5dp"
                android:text="Mal de gorge"
                android:textSize="12sp" />

            <Button
                android:id="@+id/Button213"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_margin="2dp"
                android:background="@drawable/arrondi"
                android:fontFamily="@font/maax_rounded"
                android:padding="5dp"
                android:text="Grippe"
                android:textSize="12sp" />

        </TableRow>

        <TableRow
            android:id="@+id/tableRow4"
            android:layout_width="307dp"
            android:layout_height="47dp"
            android:layout_margin="5dp"
            android:gravity="center|clip_vertical|center_horizontal|center_vertical"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tableRow6">

            <Button
                android:id="@+id/Button25"
                android:layout_width="64dp"
                android:layout_height="40dp"
                android:layout_margin="2dp"
                android:background="@drawable/arrondi"
                android:fontFamily="@font/maax_rounded"
                android:padding="5dp"
                android:text="Gastro"
                android:textSize="12sp" />

            <Button
                android:id="@+id/Button207"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_margin="2dp"
                android:background="@drawable/arrondi"
                android:fontFamily="@font/maax_rounded"
                android:padding="5dp"
                android:text="Mal de ventre"
                android:textSize="12sp" />

            <Button
                android:id="@+id/Button206"
                android:layout_width="57dp"
                android:layout_height="40dp"
                android:layout_margin="2dp"
                android:background="@drawable/arrondi"
                android:fontFamily="@font/maax_rounded"
                android:padding="5dp"
                android:text="Rhinite"
                android:textSize="12sp" />

            <Button
                android:id="@+id/Button205"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_margin="2dp"
                android:background="@drawable/arrondi"
                android:fontFamily="@font/maax_rounded"
                android:padding="5dp"
                android:text="Rhume"
                android:textSize="12sp" />
        </TableRow>

        <TableRow
            android:id="@+id/tableRow2"
            android:layout_width="311dp"
            android:layout_height="40dp"
            android:layout_margin="5dp"
            android:gravity="center"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tableRow4">

            <Button
                android:id="@+id/Button203"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_margin="2dp"
                android:background="@drawable/arrondi"
                android:fontFamily="@font/maax_rounded"
                android:padding="5dp"
                android:text="Maladies psychosomatiques "
                android:textSize="12sp" />

            <Button
                android:id="@+id/Button202"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
