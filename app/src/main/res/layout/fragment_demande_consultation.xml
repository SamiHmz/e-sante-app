                android:padding="5dp"
                android:text="Constipation   "
                android:textSize="12sp" />

            <Button
                android:id="@+id/Button220"
                android:layout_width="wrap_content"
                android:layout_height="40dp"
                android:layout_margin="2dp"
                android:background="@drawable/arrondi"
                android:fontFamily="@font/maax_rounded"
                android:gravity="center"
                android:padding="5dp"
                android:text="Irritation du reau"
                android:textSize="12sp" />
        </TableRow>

        <TextView
            android:id="@+id/textView20"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="Autre Symptômes "
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.143"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tableRow8" />

        <EditText
            android:id="@+id/editText4"
            android:layout_width="283dp"
            android:layout_height="196dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/arrondi"
            android:ems="10"
            android:fontFamily="@font/maax_rounded"
            android:gravity="top|left"
            android:hint="mentionner les autres symptomes ..."
            android:inputType="textPersonName"
            android:padding="7dp"
            android:textSize="14dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView20" />

        <TextView
            android:id="@+id/textView25"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="Traitements"
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.12"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText4" />

        <EditText
            android:id="@+id/editText5"
            android:layout_width="285dp"
            android:layout_height="222dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/arrondi"
            android:ems="10"
            android:fontFamily="@font/maax_rounded"
            android:gravity="top|left"
            android:hint="mentioner les traitement en cours pris ..."
            android:inputType="textPersonName"
            android:padding="7dp"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView25" />

        <TextView
            android:id="@+id/textView26"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="Photo"
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.108"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText5" />

        <EditText
            android:id="@+id/editText6"
            android:layout_width="282dp"
            android:layout_height="47dp"
            android:layout_marginTop="15dp"
            android:background="@drawable/arrondi"
            android:ems="10"
            android:fontFamily="@font/maax_rounded"
            android:gravity="center|left"
            android:hint="joindre une photo"
            android:inputType="textPersonName"
            android:padding="7dp"
            android:textSize="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView26" />

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="30dp"
            android:layout_height="28dp"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"
            android:src="@drawable/photo"
            app:layout_constraintBottom_toBottomOf="@+id/editText6"
            app:layout_constraintEnd_toEndOf="@+id/editText6"
            app:layout_constraintTop_toTopOf="@+id/editText6" />


        <Button
            android:id="@+id/button230"
            android:layout_width="229dp"
            android:layout_height="48dp"
            android:layout_marginTop="40dp"
            android:background="@drawable/arrondi_bleu"
            android:fontFamily="@font/maaxrounded_bold"
            android:text="Envoyer"
            android:textColor="#FFFFFF"
            android:textSize="15sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/editText6" />

        <TextView
            android:id="@+id/textView45"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:text="Demande"
            android:textSize="20dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</ScrollView>